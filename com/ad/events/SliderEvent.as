package com.ad.events {	import flash.events.Event;		public class SliderEvent extends Event {		public static const START_DRAG:String = 'SliderEvent.START_DRAG';		public static const STOP_DRAG:String = 'SliderEvent.STOP_DRAG';		public static const MOVE_DRAG:String = 'SliderEvent.MOVE_DRAG';				public function SliderEvent(type:String) { 			super(type, false, false);		}				override public function clone():Event { 			return new SliderEvent(type);		}				override public function toString():String {			return super.formatToString('SliderEvent', 'type', 'eventPhase'); 		}	}}